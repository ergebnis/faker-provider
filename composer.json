{
  "name": "ergebnis/faker-provider",
  "type": "library",
  "description": "Provides additional providers for fzaninotto/faker.",
  "keywords": [
    "fzaninotto",
    "faker",
    "provider"
  ],
  "homepage": "https://github.com/ergebnis/faker-provider",
  "license": "MIT",
  "authors": [
    {
      "name": "Andreas MÃ¶ller",
      "email": "am@localheinz.com"
    }
  ],
  "require": {
    "php": "^7.2",
    "fzaninotto/faker": "^1.9.1"
  },
  "require-dev": {
    "ergebnis/composer-normalize": "^2.2.3",
    "ergebnis/license": "~0.1.0",
    "ergebnis/php-cs-fixer-config": "^2.0.0",
    "ergebnis/phpstan-rules": "~0.14.3",
    "ergebnis/test-util": "~0.9.1",
    "infection/infection": "~0.15.2",
    "phpstan/extension-installer": "^1.0.3",
    "phpstan/phpstan": "~0.12.10",
    "phpstan/phpstan-deprecation-rules": "~0.12.2",
    "phpstan/phpstan-phpunit": "~0.12.6",
    "phpstan/phpstan-strict-rules": "~0.12.2",
    "phpunit/phpunit": "^8.5.2",
    "psalm/plugin-phpunit": "~0.9.0",
    "vimeo/psalm": "^3.9.0"
  },
  "config": {
    "platform": {
      "php": "7.2.25"
    },
    "preferred-install": "dist",
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "Ergebnis\\Faker\\Provider\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Ergebnis\\Faker\\Provider\\Test\\": "test/"
    }
  },
  "support": {
    "issues": "https://github.com/ergebnis/faker-provider/issues",
    "source": "https://github.com/ergebnis/faker-provider"
  }
}
